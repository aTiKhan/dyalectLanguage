type Lazy(Function fn) {
    var cache
}

func Lazy.GetValue() {
    if this!cache {
        return this!cache
    }
    //Get private and set private should
    //work directly through the type
    this!cache = this!fn()
    this!fn = nil
    return this!cache
}

let lz = Lazy(() => {
    print("Calculated")
    return 2 + 12.409
})

print << lz.GetValue()
print << lz.GetValue()