func takeItAll(x, y, args...) {
    var str = "x=" + y + ";y=" + y + ";other=["
    var count = 0
    for x in args {
        if count > 1 {
            str += ","
        }
        str += x
        count += 1
    }
    str += "]"
}
var x = takeItAll(12, 33.4, "oops", true, [1], (x: 0.33, y: 1))
assert("x=33.4;y=33.4;other=[oopstrue,[1],(x: 0.33, y: 1)]", x)
