#test "Constructor"
    assert(true, Bool(12))
    assert(false, Bool(0))
    assert(true, Bool("str"))
    assert(false, Bool(""))
    assert(false, Bool(nil))
    assert(true, Bool([]))
#endtest

#test "Methods: default"
    assert(false, Bool.Default())
#endtest

#test "Methods: getType"
    let x = false
    assert(true, TypeInfo(x) == Bool)
    assert(true, x.GetType() == Bool)
    var t = TypeInfo(x)
    assert(4, t.Code)
    assert("Bool", t.Name)
#endtest