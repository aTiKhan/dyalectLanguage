func eulerTest() {
    const T0 = 100.0
    const TR = 20.0
    const k = 0.07
    const n = 100
    func newtonCooling(t) {
        -k * (t - TR)
    } 
    func euler(fn, y, n, h) {
        var values = []
        var x = 0
        while x <= n {
            values.add(x + "=" + y)
            y += h * fn(y)
            x += h
        }
        values
    }
    const arr = euler(newtonCooling, T0, n, 10.0)
    var str = ""
    for e in arr {
        str += e + ";"
    }
    str
}

