func toMapTest1() {
    let xs = yields {
        (key: 1, value: "Foo"),
        (key: 2, value: "Bar"),
        (key: 3, value: "Zoo")
    }
    var map = xs.toMap(kv => kv::key, kv => kv::value)
    assert("Foo", map[1])
    assert("Bar", map[2])
    assert("Zoo", map[3])
    map = xs.toMap(kv => kv::key)
    assert((key: 1, value: "Foo"), map[1])
    assert((key: 2, value: "Bar"), map[2])
    assert((key: 3, value: "Zoo"), map[3])
}

func toMapTest2() {
    let xs = yields {
        (key: 1, value: "Foo"),
        (key: 2, value: "Bar"),
        (key: 1, value: "Zoo")
    }
    try {
        let _ = xs.toMap(kv => kv::key)
        assert(true, false)
    } catch {
        KeyAlreadyPresent() => assert(true, true)
    }
}

func emptySequence() {
    func iter() {
        yield break
    }
    var xs = iter()
    assert(0, xs.len())
    assert(nil, xs.first())
    assert(nil, xs.last())
    assert([], xs.toArray())
    xs = yields {}
    assert(0, xs.len())
    assert(nil, xs.first())
    assert(nil, xs.last())
    assert([], xs.toArray())
}