func iteratorTest1() {
    func foo(token) {
        yield 1
        yield 3
        yield 44.2
        if token {
            yield break
        }
        yield 68.6
    }
    var it = foo(false)
    var res = it.element(at: 0) + it.element(at: 1) +
        it.element(at: 2) + it.element(at: 3)
    assert(116.8, res)
}

