func tryCatchBug() { //bug #365
    var val = 0
    func iter() {
        try {
            yield 1
            throw "oops"
            yield 2
        } catch e {
            val = 42
        }
    }
    for _ in iter() { }
    assert(42, val)
}

func yieldBreakTest1() { // #399
    func seq() {
    var x = 100
    while x > 0 {
            yield x
            x -= 1
            if x == 87 {
                yield break
            }
        }
    }
    var xs = []
    for x in seq() {
        xs.add(x)
    }
    assert([100, 99, 98, 97, 96, 95, 94, 93, 92, 91, 90, 89, 88], xs)
}

