func shuffleTest() {
    func eq(xs, ys) {
        return false when xs.len() != ys.len()
        for i in 0..<xs.len() {
            if xs[i] != ys[i] {
                return false
            }
        }
        return true
    }
    let xs = yields { 1,2,3,4,5,6 }
    var ys = xs.shuffle().toArray()
    assert(6, ys.len())
    assert(true, !eq(xs.toArray(), ys))
}

