func roundTest() {
    var x = round(42.12345) //Evaluates to 42.12
    var y = round(42.12345, 4) //Evaluates to 42.1234
    assert(42.12, x)
    assert(42.1234, y)
}

func minTest() {
    var x = min(12, 2)
    assert(2, x)
    x = min(2, 24)
    assert(2, x)
    x = min(12, 12)
    assert(12, x)
    x = min(-12, 0)
    assert(-12, x)
}

func maxTest() {
    var x = max(12, 2)
    assert(12, x)
    x = max(2, 24)
    assert(24, x)
    x = max(12, 12)
    assert(12, x)
    x = max(-12, 0)
    assert(0, x)
}

func signTest() {
    var x = sign(123)
    assert(1, x)
    x = sign(-668)
    assert(-1, x)
    x = sign(0)
    assert(0, x)
}

func sqrtTest() {
    var x = sqrt(246)
    x = round(x, 4)
    assert(15.6844, x)
}

func absTest() {
    assert(12, abs(-12))
    assert(42, abs(42))
    let x = -14.102
    assert(14.102, abs(x))
}

func rndTest() {
    let x = rnd(max = 10, min = 3)
    assert(true, x <= 10 && x >= 3)
}

func parseTest() {
    assert(true, parse("true"))
    assert((1,2), parse("(1,2)"))
    assert([1,2,3], parse("[1,2,3]"))
    var xs = parse("[12.0, true, (1,2)]")
    var pat = [12.0, true, (1,2)]
    assert(pat, xs)
}

func evalTest() {
    let src = "x + y * z"
    var res = eval(src, (x = 2, y = 12, z = 3) )
    assert(38, res)
}