#test "Ackermann function"
    func ack(m, n) {
        if m > 0 {
            if n > 0 {
                ack(m - 1, ack(m, n - 1))
            } else if n == 0 {
                ack(m - 1, 1)
            } else {
                "OutOfRange"
            }
        }
        else if m == 0 && n > 0 {
            n + 1
        } else {
            "OutOfRange"
        }
    }
    var x = ack(3, 4)
    assert(125, x)
#endtest

#test "Folding a list"
    func fold(xs, fn) {
        var accum = 0
        for x in xs {
            accum = fn(accum, x)
        }
        accum
    }
    var x = fold([1,2,3,4,5,6], (acc,x) => acc + x)
    assert(21, x)
#endtest

#test "Factors of an integer 1"
    func factors(num) {
        let arr = []
        var x = 1
        while x <= num {
            if num % x == 0 {
                arr.add(x)
            }
            x += 1
        }
        arr
    }
    var x = factors(27)
    assert([1, 3, 9, 27], x)
#endtest

