#test "FizzBuzz"
    var n = 1
    var acc = ""
    while n < 11 {
        if n % 15 == 0 {
            acc = acc + "fizzbuzz"
        } else if n % 3 == 0 {
            acc = acc + "fizz"
        } else if n % 5 == 0 {
            acc = acc + "buzz"
        } else {
            acc = acc + n
        }
        n = n + 1
    }
    assert(acc, "12fizz4buzzfizz78fizzbuzz")
#endtest

#test "Factorial"
    func fact(n) {
        if n == 1 {
            1
        } else {
            n * fact(n - 1)
        }
    }
    var res = fact(20)
    assert(2432902008176640000, res)
#endtest

#test "Raise to power (recursion)"
    func pow(basis, exponent) =>
        match exponent {
            1 => basis,
            0 => 1,
            _ => basis * pow(basis, exponent - 1)
        }
    let x = pow(10, 3)
    assert(x, 1000)
#endtest

