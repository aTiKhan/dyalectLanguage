#test "Antiprime"
    func countDivisors(n) {
        if n < 2 {
            return 1
        }
        var count = 2
        var i = 2
        while i <= n / 2 {
            if n % i == 0 {
                count += 1
            }
            i += 1
        }
        count
    }
    var x = countDivisors(20)
    assert(6, x)
#endtest

#test "Names of god"
    var cache = [[1]]
    func namesOfGod(n) {
        for l in cache.len()..n {
            var r = [0]
            if l == 1 {
                r.add(r[r.len() - 1] + cache[0][0])
            } else {
                for x in 1..l {
                    r.add(r[r.len() - 1] + cache[l - x][min(x, l-x)])
                }
            }
            cache.add(r)
        }
        return cache[n]
    }
    func row(n) {
        let r = namesOfGod(n)
        var returnArray = []
        //for i in 0..(n - 1) {
        for i in 0..<n {
            returnArray.add(r[i + 1] - r[i])
        }
        return returnArray
    }
    var res = row(10)
    assert([1, 5, 8, 9, 7, 5, 3, 2, 1, 1], res)
#endtest

#test "Raise to power (cycle)"
    func pow(a, b) {
        var y = 1

        while true {
            if (b &&& 1) != 0 {
                y = a*y
            }
            b = b >>> 1
            if b == 0 {
                return y
            }
            a *= a
        }
    }
    let res = pow(12, 4)
    assert(20736, res)
#endtest

