#test "Literal and initialization"
    let x = nil
    assert(nil, x)
    var y
    assert(y, nil)
    assert(got: x == y)
#endtest

#test "Constructor"
    assert(nil, Nil())
#endtest

#test "Methods: default"
    assert(nil, Nil.default())
#endtest

#test "Methods: getType"
    let x = nil
    assert(true, TypeInfo(x) == Nil)
    assert(true, x.getType() == Nil)
    var t = TypeInfo(x)
    assert(1, t.code)
    assert("Nil", t.name)
#endtest