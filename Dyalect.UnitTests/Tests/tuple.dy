func tupleAdd() {
    var xs = (1,)
    xs = xs.add(2)
    xs = xs.add(3)
    assert((1,2,3), xs)
}

func tupleRemove() {
    var xs = (1,2,3,4,5)
    xs = xs.remove(3)
    xs = xs.remove(6)
    xs = xs.remove(5)
    assert((1,2,4), xs)
}

func tupleRemoveAt() {
    var xs = (1,2,3,4)
    xs = xs.removeAt(3)
    assert((1,2,3), xs)
    xs = xs.removeAt(1)
    assert((1,3), xs)
}

func tupleInsert() {
    var xs = (1,2,3)
    xs = xs.insert(0, 0)
    assert((0,1,2,3), xs)
    xs = xs.insert(4, 4)
    assert((0,1,2,3,4), xs)
    xs = xs.insert(3, 2.5)
    assert((0,1,2,2.5,3,4), xs)
}
