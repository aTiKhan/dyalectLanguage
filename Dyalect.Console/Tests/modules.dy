import refs.libtest lib
import refs.libtest1 lib1

func moduleTest1() {
    var abs = lib.abs(-12)
    assert(12, abs)
    assert(2, lib.abs(-2))
}

func moduleTest2() {
    var a = abs(-12)
    assert(12, a)
    var b = min(2, 1)
    assert(1, b)
}

func moduleTest3() {
    var res = match lib {
        sign: fn => fn(-12)
    }
    assert(-1, res)
}

func moduleTest4() {
    var res = lib is (id: _)
    assert(false, res)
}

func moduleTest5() {
    var res = lib1.sayHello()
    assert("Hello, User Jon", res)
}

func moduleTest6() {
    var res = lib.double(2)
    var res1 = lib1.double(2)
    assert(4, res)
    assert("double 2", res1)
}