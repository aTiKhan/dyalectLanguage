type U0()
type U1()
var bazCallCount = 0

func baz() {
    bazCallCount += 1
    if bazCallCount == 1 {
        throw U0()
    } else if bazCallCount == 2 {
        throw U1()
    }
}

func bar() {
    baz()
}

func foo() {
    var calls = 2
    while calls > 0 {
        try {
            bar()
        } catch {
            U0() => "U0 caught."
        }
        calls -= 1
    }
}

foo()