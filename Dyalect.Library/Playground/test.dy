type List = Cons(x, next) | Nil()

func List.toString() {
    func walk(xs) {
        match xs {
            Cons(x, Nil()) => x.toString(),
            Cons(x, next) => x.toString() + "," + walk(next),
            Nil() => ""
        }
    }
    return "[" + walk(this) + "]"
}

static func List.List(values...) {
    var xs = List.Nil()
    for x in values {
        xs = List.Cons(x, xs)
    }
    xs
}


